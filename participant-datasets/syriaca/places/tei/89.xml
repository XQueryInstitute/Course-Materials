                    <idno type="URI">http://pleiades.stoa.org/places/658474</idno>
                    <idno type="URI">http://en.wikipedia.org/wiki/Kahramanmaraş</idno>
                    <idno type="URI">http://dbpedia.org/resource/Kahramanmaraş</idno>
