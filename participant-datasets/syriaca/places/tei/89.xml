            <listPlace>
                <place xml:id="place-89" type="settlement">
                    <placeName xml:id="name89-1" xml:lang="en" syriaca-tags="#syriaca-headword" source="#bib89-1 #bib89-4">Marʿash</placeName>
                    <placeName xml:id="name89-2" xml:lang="en" source="#bib89-1">Germanicea</placeName>
                    <placeName xml:id="name89-3" xml:lang="syr" syriaca-tags="#syriaca-headword">ܡܪܥܫ</placeName>
                    <placeName xml:id="name89-4" xml:lang="syr-Syrj" source="#bib89-2">ܡܰܪܥܫ</placeName>
                    <placeName xml:id="name89-5" xml:lang="syr-Syrj" source="#bib89-2">ܓܰܪܡܰܐܢܝܩܝ</placeName>
                    <placeName xml:id="name89-6" xml:lang="ar" source="#bib89-3">مرعش</placeName>
                    <placeName xml:id="name89-7" xml:lang="ar" source="#bib89-3">جرمانيقي</placeName>
                    <placeName xml:id="name89-8" xml:lang="en" source="#bib89-4">Germanicia</placeName>
                    <placeName xml:id="name89-9" xml:lang="en" source="#bib89-4">Kahramanmaraş</placeName>
                    <desc xml:id="abstract-en-89" xml:lang="en">A city near the <placeName ref="http://syriaca.org/place/159">Pyramos river</placeName> northeast of <placeName ref="http://syriaca.org/place/9">ʿAyn Zarba</placeName> and northwest of <placeName ref="http://syriaca.org/place/222">ʿAintab</placeName>.</desc>
                    <desc xml:lang="syr-Syrj">
                        <quote source="#bib89-2">ܘܐܝܬܶܝܗ̇ ܓܰܪܡܰܐܢܝܩܝ ܥܰܬܝܩܬܐ ܡܕܝܺܢܬܐ ܕܰܒܓܰܪܒܝ ܚܳܠܳܒ ܘܰܒܬܝܡܢܳܐ ܕܣܺܝܘܐܰܣ</quote>
                    </desc>
                    <desc xml:lang="ar">
                        <quote source="#bib89-3">وهي جرمانيقي القديمة؛ مدينة شمالي حلب وجنوبي سيواس.</quote>
                    </desc>
                    <desc xml:lang="en">
                        <quote source="#bib89-4">the ancient Germanicia, north of Aleppo and south of Sivas. [Modern name, Kahramanmaraş. (tr.)]</quote>
                    </desc>
                    <location type="gps" source="#bib89-1">
                        <geo>37.583333 36.933333</geo>
                    </location>
                    <state type="existence" from="0710" to="2013" syriaca-computed-end="2013-01-01" syriaca-computed-start="0710-01-01">
                        <precision match="@from" notAfter="0710" syriaca-computed-end="0710-01-01"/>
                        <precision match="@to" notBefore="2013" syriaca-computed-start="2013-01-01"/>
                    </state>
